{
	"languageserver": {
		"go": {
			"command": "gopls",
			"rootPatterns": [
				"go.mod"
			],
			"trace.server": "verbose",
			"filetypes": [
				"go"
			]
		},
		"efm": {
			"command": "efm-langserver",
			"args": [],
			"filetypes": [
				"vim",
				"eruby",
				"markdown",
				"yaml"
			]
		}
	},
	"suggest.noselect": true,
	"flutter.lsp.initialization.closingLabels": true
}
